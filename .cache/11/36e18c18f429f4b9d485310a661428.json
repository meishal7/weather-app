umn":0}},{"source":"style.css","original":{"line":369,"column":0},"generated":{"line":369,"column":0}}],"sources":{"style.css":"html {\n  box-sizing: border-box;\n  font-size: 62.5%;\n}\n*,\n*:before,\n*:after {\n  box-sizing: inherit;\n}\n:root {\n  --bg-main: #fffafa;\n  --border-color: #ff9bad;\n  --card-gradient: linear-gradient(\n    266.69deg,\n    rgba(206, 134, 252, 0.25) 23.78%,\n    rgba(255, 134, 140, 0.25) 78.77%\n  );\n  --text-color: #00000073;\n  --box-shadow: 0px 4px 6px 4px #0000000c;\n}\nbody {\n  color: var(--text-color);\n  font-family: \"Rubik\", sans-serif;\n}\n\n.wrapper {\n  margin: 0 auto;\n  max-width: 500px;\n  position: relative;\n}\n.spinner {\n  position: absolute;\n  width: 100%;\n  height: 100%;\n  background-color: teal;\n  color: white;\n  display: none;\n}\n.loading {\n  display: block;\n}\n\n/* Hour weather style */\n\n.card {\n  background: var(--card-gradient);\n  border-radius: 6px;\n  padding: 1.5rem;\n  margin: 1.5rem;\n  box-shadow: var(--box-shadow);\n}\n.card p {\n  margin: 0;\n}\n.hour-weather {\n  display: flex;\n}\n.hour-weather-list {\n  display: flex;\n  scroll-behavior: smooth;\n  gap: 15px;\n  list-style-type: none;\n  padding-left: 0;\n  justify-content: space-between;\n  overflow-x: auto;\n  scroll-snap-type: x mandatory;\n  padding-bottom: 15px;\n  margin: 0;\n  margin-bottom: -15px;\n  scrollbar-width: none;\n}\n.hour-weather-list::-webkit-scrollbar {\n  display: none;\n}\n\n.hour-weather-list > li {\n  scroll-snap-align: start;\n  flex: 1 0 50px;\n  text-align: center;\n}\n.hour-weather-list > li img {\n  margin: 0.5rem 0;\n  height: 30px;\n}\n.hour-weather-list > li > p {\n  font-family: Rubik;\n  font-style: normal;\n  font-weight: normal;\n  font-size: 16px;\n  line-height: 19px;\n  color: rgba(0, 0, 0, 0.45);\n}\n/* Week weather style */\n.week-weather-list {\n  list-style-type: none;\n  padding-left: 0;\n}\n.week-weather-list > li {\n  display: grid;\n  grid-template-columns: 85px 30px 1fr 1fr 1fr;\n  gap: 1rem;\n  padding: 1rem 0;\n  border-bottom: 1px solid var(--border-color);\n  align-items: center;\n  font-size: 1.6rem;\n}\n\n.week-weather-list img {\n  display: block;\n  max-width: 100%;\n  max-height: 25px;\n}\n.week-weather-list > li > div {\n  display: flex;\n  align-items: center;\n  gap: 5px;\n}\n\n.week-weather-list > li > p {\n  font-family: Rubik;\n  font-style: normal;\n  font-weight: normal;\n  font-size: 16px;\n  line-height: 19px;\n  color: rgba(0, 0, 0, 0.45);\n  display: inline-block;\n  grid-column-start: 1;\n  grid-column-end: 1;\n}\n/*Current weather style */\n.current-weather {\n  background-size: cover;\n  background-repeat: no-repeat;\n  background-position: 0% 25%;\n  height: 200px;\n  border-bottom-left-radius: 81% 15%;\n  border-bottom-right-radius: 81% 15%;\n  box-shadow: inset 0px -6px 4px rgba(0, 0, 0, 0.25);\n}\n\n.curr-weather-city {\n  text-align: center;\n  font-style: normal;\n  font-weight: 500;\n  font-size: 36px;\n  line-height: 43px;\n  margin: 0 0;\n  padding-top: 2.5rem;\n  color: #fffafa;\n  text-shadow: 0px 4px 4px rgba(0, 0, 0, 0.1);\n}\n.curr-weather-temp {\n  text-align: center;\n  font-style: normal;\n  font-weight: 500;\n  font-size: 36px;\n  line-height: 43px;\n  margin: 0 0;\n  color: #ffffff;\n  text-shadow: 0px 4px 4px rgba(0, 0, 0, 0.1);\n}\n.curr-weather-condition {\n  padding-left: 1.5rem;\n  font-style: normal;\n  font-weight: normal;\n  font-size: 18px;\n  line-height: 21px;\n  margin: 0 0;\n  color: #ffffff;\n}\n.curr-weather-min {\n  padding-left: 1.5rem;\n  font-style: normal;\n  font-weight: normal;\n  font-size: 18px;\n  line-height: 21px;\n  color: #ffffff;\n  display: inline-block;\n  margin: 0 0;\n}\n.curr-weather-max {\n  font-style: normal;\n  font-weight: normal;\n  font-size: 18px;\n  line-height: 21px;\n  color: #ffffff;\n  display: inline-block;\n  padding-left: 1rem;\n  margin: 0 0;\n  padding-top: 0.5rem;\n}\n\n/* Current weather details */\n.current-weather-details {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  margin-left: 1.5rem;\n  margin-right: 1.5rem;\n  grid-column-gap: 1rem;\n  grid-row-gap: 1rem;\n}\n.current-weather-details > div {\n  margin: 0;\n  padding: 1.5rem;\n  display: flex;\n}\n\n.card-image > img {\n  width: 50px;\n  max-height: 40px;\n  object-fit: contain;\n}\n.card-value {\n  flex: 1;\n}\n.card-value > p {\n  font-family: Rubik;\n  font-style: normal;\n  font-weight: normal;\n  font-size: 18px;\n  line-height: 21px;\n  text-align: right;\n\n  color: rgba(0, 0, 0, 0.45);\n}\n.card-value p + p {\n  font-size: 12px;\n}\n/* Footer style */\n.footer {\n  border-top-left-radius: 50% 25%;\n  border-top-right-radius: 50% 25%;\n  padding: 10px 0;\n  background: linear-gradient(\n    267.08deg,\n    rgba(139, 45, 124, 1) 23.58%,\n    rgba(58, 22, 163, 1) 63.72%\n  );\n  box-shadow: inset 0px 4px 4px rgba(0, 0, 0, 0.25);\n  margin-top: 2rem;\n  height: 60px;\n  position: sticky;\n  overflow: hidden;\n  bottom: 0;\n  right: 0;\n  width: 100%;\n  transition: all ease-in-out 0.3s;\n}\n.footer > button {\n  margin: 0 auto;\n  display: block;\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n  border: 1px solid white;\n  border-radius: 1rem;\n  padding: 0.8rem 1rem;\n}\n.footer-open {\n  border-top-left-radius: 50% 10%;\n  border-top-right-radius: 50% 10%;\n  height: 600px;\n}\n.footer img:last-of-type {\n  display: none;\n}\n.footer.footer-open img:first-of-type {\n  display: none;\n}\n.footer.footer-open img:last-of-type {\n  display: block;\n}\n\n.pac-item-query:hover {\n  color: black;\n}\n.pac-item-query-selected {\n  color: black;\n}\n\n.pac-target-input {\n  animation-name: endBrowserAutofill;\n  border: white 1 px solid;\n  border-radius: 4px;\n\n  color: black;\n  font-family: \"Rubik\", sans-serif;\n  width: 150px;\n  margin-left: 30px;\n  margin-bottom: 2rem;\n  border: white 1px solid;\n  border-radius: 4px;\n  padding: 0.5rem;\n}\n\n.search-span {\n  color: #fffafa;\n  margin-left: 30px;\n  margin-top: 30px;\n  margin-bottom: 5px;\n  display: block;\n}\n.saved-locations {\n  max-height: 450px;\n  overflow: auto;\n  scrollbar-width: none;\n}\n.saved-locations::-webkit-scrollbar {\n  display: none;\n}\n.saved-loc-card {\n  margin: 10px 30px;\n  padding: 15px;\n  border-radius: 10px;\n  font-family: Rubik;\n  font-style: normal;\n  font-weight: normal;\n  font-size: 16px;\n  line-height: 19px;\n  color: #fffafa;\n  text-shadow: 0px 4px 4px rgba(0, 0, 0, 0.1);\n}\n.saved-loc-card div {\n  display: flex;\n  justify-content: space-between;\n}\n.saved-loc-card div p {\n  margin: 0 0;\n  padding-top: 0.5rem;\n  font-size: 25px;\n}\n.saved-loc-time {\n  margin: 0 0;\n}\ninput:focus {\n  outline: none;\n}\n.search-and-temp-switch {\n  display: flex;\n  justify-content: space-between;\n}\n.temp-buttons-div {\n  margin-top: 5rem;\n  margin-right: 3rem;\n}\n.temp-btn {\n  border: none;\n  background: transparent;\n  color: #b6b6b6;\n  font-size: 20px;\n}\n.temp-buttons-div span {\n  color: #b6b6b6;\n  font-size: 20px;\n}\n.active-btn {\n  color: #fffafa;\n}\n.arrow-btn {\n  border: none;\n  background: transparent;\n  font-size: 5rem;\n  color: white;\n  text-shadow: 0px 4px 4px rgba(0, 0, 0, 0.1);\n}\n.no-scroll {\n  overflow: hidden;\n}\n"},"lineCount":369}},"error":null,"hash":"5c1cd8d9fbafaf652b1d7830224233fc","cacheData":{"env":{}}}