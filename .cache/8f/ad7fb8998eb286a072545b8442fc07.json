ta","generated":{"line":64,"column":51},"source":"modules/refreshSavedLocations.js","original":{"line":49,"column":51}},{"generated":{"line":64,"column":55},"source":"modules/refreshSavedLocations.js","original":{"line":49,"column":36}},{"generated":{"line":64,"column":56},"source":"modules/refreshSavedLocations.js","original":{"line":49,"column":2}},{"generated":{"line":65,"column":0},"source":"modules/refreshSavedLocations.js","original":{"line":50,"column":1}}],"sources":{"modules/refreshSavedLocations.js":"// refreshSavedLocations()\n// This function is called every time the app is loaded.\n// It refreshes info for saved locations in local storage\nimport getWeather from \"./getWeather\";\n\nconst apiKey = process.env.KEY;\n\nexport default function refreshSavedLocations(defaultDegree) {\n  const data = JSON.parse(localStorage.getItem(\"locations\"));\n  if (!data) return;\n  data.cities.forEach(async (el) => {\n    //console.log(\"refreshed\");\n    const weatherData = await getWeather(\n      el.lattitude,\n      el.longtitude,\n      defaultDegree\n    );\n    // Convert EPOC time to normal time\n    let savedLocTimeConv = new Date(weatherData.current.dt * 1000);\n    let savedLocTimeArray = savedLocTimeConv.toLocaleTimeString().split(\":\");\n    let savedLocTime =\n      savedLocTimeArray[0] +\n      \".\" +\n      savedLocTimeArray[1] +\n      savedLocTimeArray[2].charAt(3) +\n      savedLocTimeArray[2].charAt(4);\n    // Determine if it's day or night for choosing background image\n    let dayTime = \"\";\n    if (\n      weatherData.current.dt > weatherData.current.sunrise &&\n      weatherData.current.dt < weatherData.current.sunset\n    ) {\n      dayTime = \"day\";\n    } else if (\n      weatherData.current.dt > weatherData.current.sunrise &&\n      weatherData.current.dt > weatherData.current.sunset\n    ) {\n      dayTime = \"night\";\n    } else if (weatherData.current.dt < weatherData.current.sunrise) {\n      dayTime = \"night\";\n    }\n    el.temp = Math.floor(weatherData.current.temp);\n    (el.time = savedLocTime),\n      (el.condition = weatherData.current.weather[0].main),\n      (el.dayTime = dayTime);\n    //console.log(data);\n  });\n  data.defaultDegree = defaultDegree;\n  localStorage.setItem(\"locations\", JSON.stringify(data));\n}\n"},"lineCount":null}},"error":null,"hash":"2fe1345fe54c323c7df2c9adecceda8d","cacheData":{"env":{"KEY":"74a29df014963a5cf46387efc2a24cc3"}}}